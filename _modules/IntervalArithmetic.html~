<div class="viewcode-block" id="IntervalInt2D"><a class="viewcode-back" href="../api/IntervalArithmetic.html#IntervalArithmetic.IntervalInt2D">[docs]</a><span class="k">class</span> <span class="nc">IntervalInt2D</span><span class="p">(</span><span class="n">Interval2D</span><span class="p">):</span>

    <span class="sd">&quot;&quot;&quot; Interval [inf, sup]*[inf, sup] in the integer domain.</span>
<span class="sd">    &quot;&quot;&quot;</span>

    <span class="c1">##############################################</span>

    <span class="k">def</span> <span class="nf">__init__</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">x</span><span class="p">,</span> <span class="n">y</span><span class="p">):</span>

        <span class="bp">self</span><span class="o">.</span><span class="n">x</span> <span class="o">=</span> <span class="n">IntervalInt</span><span class="p">(</span><span class="n">x</span><span class="p">)</span>
        <span class="bp">self</span><span class="o">.</span><span class="n">y</span> <span class="o">=</span> <span class="n">IntervalInt</span><span class="p">(</span><span class="n">y</span><span class="p">)</span>

    <span class="c1">##############################################</span>

<div class="viewcode-block" id="IntervalInt2D.iter"><a class="viewcode-back" href="../api/IntervalArithmetic.html#IntervalArithmetic.IntervalInt2D.iter">[docs]</a>    <span class="k">def</span> <span class="nf">iter</span><span class="p">(</span><span class="bp">self</span><span class="p">):</span>

        <span class="k">for</span> <span class="n">x</span> <span class="ow">in</span> <span class="bp">self</span><span class="o">.</span><span class="n">x</span><span class="o">.</span><span class="n">iter</span><span class="p">():</span>
            <span class="k">for</span> <span class="n">y</span> <span class="ow">in</span> <span class="bp">self</span><span class="o">.</span><span class="n">y</span><span class="o">.</span><span class="n">iter</span><span class="p">():</span>
                <span class="k">yield</span> <span class="p">(</span><span class="n">x</span><span class="p">,</span> <span class="n">y</span><span class="p">)</span></div></div>
</pre></div>

           </div>
