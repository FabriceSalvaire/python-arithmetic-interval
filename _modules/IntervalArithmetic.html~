    <span class="k">def</span> <span class="nf">__and__</span><span class="p">(</span><span class="n">i1</span><span class="p">,</span> <span class="n">i2</span><span class="p">):</span>

        <span class="sd">&quot;&quot;&quot; Return the intersection of *i1* and *i2*.</span>

<span class="sd">        Return an empty interval if they don&#39;t intersect.</span>
<span class="sd">        &quot;&quot;&quot;</span>

        <span class="k">return</span> <span class="n">i1</span><span class="o">.</span><span class="vm">__class__</span><span class="p">(</span><span class="n">i1</span><span class="o">.</span><span class="n">x</span> <span class="o">&amp;</span> <span class="n">i2</span><span class="o">.</span><span class="n">x</span><span class="p">,</span> <span class="n">i1</span><span class="o">.</span><span class="n">y</span> <span class="o">&amp;</span> <span class="n">i2</span><span class="o">.</span><span class="n">y</span><span class="p">)</span>

    <span class="c1">##############################################</span>

    <span class="k">def</span> <span class="nf">__iand__</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">i2</span><span class="p">):</span>

        <span class="sd">&quot;&quot;&quot; Update the interval with its intersection with *i2*.</span>

<span class="sd">        Return an empty interval if they don&#39;t intersect.</span>
<span class="sd">        &quot;&quot;&quot;</span>

        <span class="bp">self</span><span class="o">.</span><span class="n">x</span> <span class="o">&amp;=</span> <span class="n">i2</span><span class="o">.</span><span class="n">x</span>
        <span class="bp">self</span><span class="o">.</span><span class="n">y</span> <span class="o">&amp;=</span> <span class="n">i2</span><span class="o">.</span><span class="n">y</span>
        <span class="k">return</span> <span class="bp">self</span>

    <span class="c1">##############################################</span>

    <span class="k">def</span> <span class="nf">__or__</span><span class="p">(</span><span class="n">i1</span><span class="p">,</span> <span class="n">i2</span><span class="p">):</span>

        <span class="sd">&quot;&quot;&quot; Return the union of *i1* and *i2*. &quot;&quot;&quot;</span>

        <span class="k">return</span> <span class="n">i1</span><span class="o">.</span><span class="vm">__class__</span><span class="p">(</span><span class="n">i1</span><span class="o">.</span><span class="n">x</span> <span class="o">|</span> <span class="n">i2</span><span class="o">.</span><span class="n">x</span><span class="p">,</span> <span class="n">i1</span><span class="o">.</span><span class="n">y</span> <span class="o">|</span> <span class="n">i2</span><span class="o">.</span><span class="n">y</span><span class="p">)</span>

    <span class="c1">##############################################</span>

    <span class="k">def</span> <span class="nf">__ior__</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">i2</span><span class="p">):</span>

        <span class="sd">&quot;&quot;&quot; Update the interval with its union with *i2*. &quot;&quot;&quot;</span>

        <span class="bp">self</span><span class="o">.</span><span class="n">x</span> <span class="o">|=</span> <span class="n">i2</span><span class="o">.</span><span class="n">x</span>
